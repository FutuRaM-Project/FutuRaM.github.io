
digraph G {
    graph [bgcolor="#ffffff"];
    node [shape=rectangle, style="filled", fillcolor="#ffffff", fontname="Arial", fontsize=12];
    edge [color="#000000", fontname="Arial", fontsize=12];
    
    subgraph cluster_develop {
        label = "";
        style = "filled";
        fillcolor = "#fcfcfc00";
        
        Start [label="BEGIN SCENARIO DEVELOPMENT", shape=ellipse, fillcolor="#9e89fdc9", style="filled", fontcolor="#ffffff"];
        DefineScope [label="Step 1: Define Scope and Objectives"];
        DetermineMethod [label="Step 2: Determine Methodology"];
        MarkerScenarioMapping [label="Step 3: Marker Scenario Mapping"];
        IdentifyDrivers [label="Step 4: Identify Key Drivers"];
        DevelopThemes [label="Step 5: Develop Storyline Themes"];
        DefineParameters [label="Step 7: Define Scenario Parameters"];
        QualNarr [label="Step 6: Qualitative Narrative Development"];
        QualitativeDevelopment [label="Step 8: Quantitative Modelling"];
        Refinement [label="Step 11. Iterative Refinement", fillcolor="#76e2372a", style="filled"];
        Communicate [label="Communication and Engagement", fillcolor="#76e23783", style="filled"];
        Monitor [label="Monitoring and Adaptation", fillcolor="#76e23788", style="filled"];
        End [label="Step 9. IMPLEMENTATION", shape=ellipse, fillcolor="#9e89fdda", style="filled", fontcolor="#ffffff"];
        
        Start -> DefineScope;
        DefineScope -> DetermineMethod;
        DetermineMethod -> MarkerScenarioMapping;
        MarkerScenarioMapping -> IdentifyDrivers;
        IdentifyDrivers -> DevelopThemes;
        DevelopThemes -> QualNarr;
        DefineParameters -> QualitativeDevelopment;
        QualNarr -> DefineParameters;
        QualitativeDevelopment -> End;
        Review -> Communicate;
        Review -> Monitor;
        Communicate -> Refinement;
        Monitor -> Refinement;
        Refinement -> Develop;
    }
    
    Review [label="Step 10: Review and Update", fillcolor="#76e2372a", style="filled"];
    
    subgraph cluster_overview {
        label = " ";
        Develop [label=" ", style="filled", fillcolor="#fcfcfc00"];
        Refinement [label=" ", style="filled", fillcolor="#76e2372a"];
        Communicate [label=" ", style="filled", fillcolor="#76e23783"];
        Monitor [label=" ", style="filled", fillcolor="#76e23788"];
        Develop -> Refinement;
        Develop -> Communicate;
        Develop -> Monitor;
        Refinement -> Review;
        Communicate -> Review;
        Monitor -> Review;
    }
}
